{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["src"],
    "includePattern": ".jsx$",
    "excludePattern": "(node_modules/|docs)"
  },

  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },

  "opts": {
    "destination": "./jsdoc",
    "recurse": true
  }
}
